#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/cory-ubuntu/coding/projects/env-manager/node_modules/.pnpm/snyk@1.1298.3/node_modules/snyk/bin/node_modules:/home/cory-ubuntu/coding/projects/env-manager/node_modules/.pnpm/snyk@1.1298.3/node_modules/snyk/node_modules:/home/cory-ubuntu/coding/projects/env-manager/node_modules/.pnpm/snyk@1.1298.3/node_modules:/home/cory-ubuntu/coding/projects/env-manager/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/cory-ubuntu/coding/projects/env-manager/node_modules/.pnpm/snyk@1.1298.3/node_modules/snyk/bin/node_modules:/home/cory-ubuntu/coding/projects/env-manager/node_modules/.pnpm/snyk@1.1298.3/node_modules/snyk/node_modules:/home/cory-ubuntu/coding/projects/env-manager/node_modules/.pnpm/snyk@1.1298.3/node_modules:/home/cory-ubuntu/coding/projects/env-manager/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../snyk/bin/snyk" "$@"
else
  exec node  "$basedir/../snyk/bin/snyk" "$@"
fi
